    "jest": "^29.6.1",
    "joi": "^17.9.2",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.1",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "standard-http-error": "^2.0.1",
